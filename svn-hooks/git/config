[core]
	repositoryformatversion = 0
	filemode = true
	bare = true
	logallrefupdates = true
[svn-remote "svn"]
        url = svn://svn.twistedmatrix.com/svn/Twisted
        fetch = trunk:refs/heads/trunk
        branches = branches/*:refs/svn/*
        tags = tags/releases/*:refs/tags/*
[svn-remote "svn-releases"]
        url = svn://svn.twistedmatrix.com/svn/Twisted
        branches = branches/releases/*:refs/heads/releases/*
[remote "mirror-prep"]
	url = .
	push = refs/svn/*:refs/mirror/*
[remote "mirror"]
	url = git@github.com:tomprince/twisted.git
	push = refs/mirror/*:refs/heads/*
	push = refs/heads/trunk
	push = refs/tags/*
	push = refs/heads/releases/*
	fetch = refs/heads/*:refs/svn/*
	fetch = refs/heads/trunk:refs/heads/trunk
	fetch = refs/tags/*:refs/tags/*
	fetch = refs/heads/releases/*:refs/heads/releases/*
